{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src",
    "**/*.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts"
  ],
  "exclude": ["node_modules", "src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
